<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.baseclean">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".application.BaseCleanApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_tmdb_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_tmdb_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BaseClean">
        <activity android:name=".ui.tvshow.TvShowActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>